{
  "name": "torrent-stream-vlc",
  "version": "0.1.1",
  "description": "Insert magnet link to stream torrent files to VLC (VideoLAN Client)",
  "main": "torrent-stream-vlc.dist.js",
  "scripts": {
    "install-dev": "npm install babel-cli@6.18.0 babel-preset-es2015@6.18.0 uglify-js@2.7.5",
    "dev": "babel torrent-stream-vlc.src.js --watch --out-file torrent-stream-vlc.dist.js",
    "dist": "uglifyjs torrent-stream-vlc.dist.js -o torrent-stream-vlc.dist.js"
  },
  "keywords": [
    "vlc",
    "torrent",
    "stream",
    "torrent-stream"
  ],
  "author": "Jan Gunzenhauser (http://jg7.co)",
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JanGunzenhauser/torrent-stream-vlc"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "ignore": [
      "node_modules/**/*"
    ]
  },
  "dependencies": {
    "mime": "1.3.4",
    "numeral": "2.0.4",
    "pump": "1.0.2",
    "range-parser": "1.2.0",
    "torrent-stream": "1.0.3"
  }
}
